<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.dfki.de/lt/onto/common/dialogue.owl#"
     xml:base="http://www.dfki.de/lt/onto/common/dialogue.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:dial="http://www.dfki.de/lt/onto/common/dialogue.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#">
    <owl:Ontology rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">author(s): Hans-Ulrich Krieger, Stefania Racioppa</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">v0.9.6</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#measure -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#measure"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#monetary -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#monetary"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#addressee -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#addressee">
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Agent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the addressee(s) of the dialogue act; in case several actors need to be specified, we usually &quot;bundle&quot; them in a container (group) object whose definition should come from an upper or domain ontology;
sometimes called &apos;speaker&apos;; not used at the moment !!</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#follows -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#follows">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">follows links the current dialogue act with its _immediate_ predecessors (a transitive property)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#frame -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#frame">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Frame"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the frame which encodes the (shallow) semantics/the semantic content of 
    the dialogue act; in principle, this property should be part of a 
    domain-specific ontology which interfaces dialogue acts and FrameNet 
    frames through the property frame, by defining a domain (viz., 
    dial:DialogueAct) and a range (viz., sem:Frame) restriction</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#hasArgs -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#hasArgs">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Pair"/>
    </owl:ObjectProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#participant -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#participant">
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Agent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">further participants (besides &apos;sender&apos; and &apos;addressee&apos;), being typed to Agent to even address groups;
called &apos;side-participants&apos;, &apos;bystanders&apos;, and &apos;overhearers&apos; by Clark (1996)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#refersTo -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#refersTo">
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">refersTo implements a kind of &apos;indirect speech&apos; when used with DAs DeclineXXX, RejectXXX, and RevokeXXX: these are dialogue acts on which the _current_ DA depends on; DIT++ sometimes call those former DAs a &apos;antecedent&apos; DA</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#sender -->

    <owl:ObjectProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#sender">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Agent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the sender who initiates the dialogue act and who wants to influence the addressee; sometimes called &apos;hearer&apos; or &apos;recipient&apos;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#first -->

    <owl:DatatypeProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#first">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Pair"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#fromTime -->

    <owl:DatatypeProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#fromTime">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">happens is employed to encode a time stamp, thus able to establish a partial order over DAs; through the happens property, we are able to assign several DAs to the _same_ utterance</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#repr -->

    <owl:DatatypeProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#repr">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A string representation of the DialogueAct to be used for more efficient processing</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#second -->

    <owl:DatatypeProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#second">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Pair"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#toTime -->

    <owl:DatatypeProperty rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#toTime">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Accept -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#AggregateFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AcceptOffer -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AcceptOffer">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#AddressOffer"/>
        <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DeclineOffer"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AcceptRequest -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AcceptRequest">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#AddressRequest"/>
        <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DeclineRequest"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AcceptSuggestion -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AcceptSuggestion">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#AddressSuggestion"/>
        <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DeclineSuggestion"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#ActionDiscussionFunction -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#ActionDiscussionFunction">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#GeneralPurposeFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AddressOffer -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AddressOffer">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Instruct"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AddressRequest -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AddressRequest">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Promise"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AddressSuggestion -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AddressSuggestion">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Commissive"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Advice -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Advice">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Directive"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;p style=&quot;margin-top: 0&quot;&gt;
      a suggestion, not part of DIT++
    &lt;/p&gt;</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Agent -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Agent"/>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AggregateFunction -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AggregateFunction">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#DialogueAct"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;p style=&quot;margin-top: 0&quot;&gt;
      this subclass of DialogueAct is intended to cross-classify dialogue acts 
      which are subclasses of GeneralPurposeFunction; the idea behind this is 
      to address uncertainty in the recognition of &amp;quot;related&amp;quot; dialogue acts 
      through a _single_ class;
    &lt;/p&gt;
    &lt;p style=&quot;margin-top: 0&quot;&gt;
      currently, we suggest to have three subclasses of AggregateFunction for 
      which I list their subclasses below:
    &lt;/p&gt;
    &lt;p style=&quot;margin-top: 0&quot;&gt;
      * Accept &amp;gt; 
      {AcceptOffer,AcceptSuggestion,AcceptRequest,Confirm,CheckPositive,Agreement}
    &lt;/p&gt;
    &lt;p style=&quot;margin-top: 0&quot;&gt;
      * Decline &amp;gt; 
      {DeclineOffer,DeclineSuggestion,DeclineRequest,Disconfirm,CheckNegative,Disagreement}
    &lt;/p&gt;
    &lt;p style=&quot;margin-top: 0&quot;&gt;
      * Revoke &amp;gt; {Revocation,Correction,RevokeSuggestion}
    &lt;/p&gt;</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Agreement -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Agreement">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Inform"/>
        <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Disagreement"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AlloFeedback -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AlloFeedback">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#FeedbackFunction"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;p style=&quot;margin-top: 0&quot;&gt;
      Allo-Feedback acts are about the speaker&apos;s beliefs about the addressee&apos;s 
      attention and processing of an utterance in the speaker&apos;s last turn (see 
      http://dit.uvt.nl/#dit_allo-fb-functions)
    &lt;/p&gt;</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AlloNegative -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AlloNegative">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#FeedbackGivingFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AlloPositive -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AlloPositive">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#FeedbackGivingFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Answer -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Answer">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Inform"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Apologizing -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Apologizing">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#SocialObligationManagementFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Apology -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Apology">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Apologizing"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#ApologyDownplay -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#ApologyDownplay">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Apologizing"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Appeasement -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Appeasement">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#SocialFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AutoFeedbackFunction -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AutoFeedbackFunction">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#FeedbackFunction"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;p style=&quot;margin-top: 0&quot;&gt;
      Auto-Feedback acts are about the speaker&apos;s own attention and processing 
      of an utterance in the addressee&apos;s last turn (see 
      http://dit.uvt.nl/#dit_auto-fb-functions)
    &lt;/p&gt;</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AutoNegative -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AutoNegative">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#AutoFeedbackFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#AutoPositive -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#AutoPositive">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#AutoFeedbackFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Breaking -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Breaking">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#TimeManagementFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#CheckNegative -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#CheckNegative">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#CheckQuestion"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Decline"/>
        <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#CheckPositive"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#CheckPositive -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#CheckPositive">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#CheckQuestion"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#CheckQuestion -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#CheckQuestion">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#YNQuestion"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#ChoiceQuestion -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#ChoiceQuestion">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Question"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Commissive -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Commissive">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#ActionDiscussionFunction"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Confirm -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Confirm">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Accept"/>
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Answer"/>
        <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Disconfirm"/>
    </owl:Class>
    


    <!-- http://www.dfki.de/lt/onto/common/dialogue.owl#Confirming -->

    <owl:Class rdf:about="http://www.dfki.de/lt/onto/common/dialogue.owl#Confirming">
        <rdfs:subClassOf rdf:resource="http://www.dfki.de/lt/onto/common/dialogue.owl#Connecting"/>
    </owl:Class>
    


