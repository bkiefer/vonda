<?xml version="1.0" encoding="UTF-8"?>
<configuration status="warn">
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="warn">
    <appender-ref ref="console"/>
  </root>
</configuration>
